<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<ThingDef ParentName="OutlandHatMakeableBase">
		<defName>Outland_HaygrassHat</defName>
		<description>A typical Eastborn Empire hat.</description>
		<label>eastborn haygrass hat</label>
		<graphicData>
			<texPath>Outland/Eastborn/Apparel/StrawHat/Head</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<costStuffCount>1</costStuffCount>
		<stuffCategories>
			<li>Woody</li>
		</stuffCategories>
		<costList>
			<Hay>25</Hay>
		</costList>
		<statBases>
			<MaxHitPoints>200</MaxHitPoints>
			<WorkToMake>600</WorkToMake>
			<Mass>0.14</Mass>
			<StuffEffectMultiplierArmor>0.2</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.10</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.50</StuffEffectMultiplierInsulation_Heat>
			<EquipDelay>0.8</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">0.05</SlaveSuppressionOffset>
		</equippedStatOffsets>
		<apparel>
			<countsAsClothingForNudity>false</countsAsClothingForNudity>
			<bodyPartGroups>
				<li>UpperHead</li>
			</bodyPartGroups>
			<wornGraphicPath>Outland/Eastborn/Apparel/StrawHat/Head</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>EastbornBasic</li>
			</tags>
			<defaultOutfitTags>
				<li>Worker</li>
			</defaultOutfitTags>
		</apparel>
		<recipeMaker>
			<useIngredientsForColor>false</useIngredientsForColor>
		</recipeMaker>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li>
					<weight>10</weight>
					<only>(199, 200, 99)</only>
				</li>
			</options>
		</colorGenerator>
	</ThingDef>

</Defs>